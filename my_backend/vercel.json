{   
    "version":2,
    "builds": [{
        "src": "my_backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },{
        "src":"build_file.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles.build"
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "my_backend/wsgi.py"
        }
    ]
}